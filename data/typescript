as[35;43Hde[35;46Hguerra)[36;3H(B[m[38;5;60;48;5;232m0[36;5H(B[m[97;48;5;232m-# Dica: Filipos[36;22Hera[36;26Huma[36;30Hcol√¥nia[36;38Hromana[36;45He[36;47Hseus[36;52Hhabitantes[36;63Htinham[36;70Hmuito[36;76Ho[37;3H(B[m[38;5;60;48;5;232m1[37;5H(B[m[97;48;5;232m   [37;9H      [37;16H  [37;19H        [37;28H   [37;32H  [37;35H         [37;45H      [37;52H   [37;56H            [37;69H    [37;74H   [38;3H(B[m[38;5;60;48;5;232m2[38;5H(B[m[97;48;5;232mid:[38;9H8d96655e70      [38;26H      [38;33H      [38;40H   [38;44H     [39;3H(B[m[38;5;60;48;5;232m3[39;5H(B[m[97;48;5;232m###[39;9HQuando os crist√£os n√£o se intimidam di[39;48Hnte[39;52Hdos advers√°rios,[39;69Hisso[39;74Hser[40;3H(B[m[38;5;60;48;5;232m4[40;16H(B[m[97;48;5;232ma justi√ßa humana falhou com ambos   [40;53H  [40;56H      [41;3H(B[m[38;5;60;48;5;232m5[41;6H(B[m[38;5;141;48;5;232mx[41;12H(B[m[97;48;5;232mperdi√ß√£o para os advers√°rios e[41;43Hsalva√ß√£o para[41;57Hos[41;60Hcrist√£os[42;3H(B[m[38;5;60;48;5;232m6[42;5H(B[m[38;5;141;48;5;232m[ ](B[m[97;48;5;232m De que[42;16Hos crist√£os possuem melhore[42;45Ht√°tic[42;51Hs de[42;58Hfesa       [42;70H    [42;75H  [43;3H(B[m[38;5;60;48;5;232m7[43;5H(B[m[38;5;141;48;5;232m[ ][43;9H(B[m[97;48;5;232mDe[43;12Hque[43;16Ha[43;18Hpersegui√ß√£o[43;30Hterminar√°[43;40Hem[43;43Hcurto[43;49Hprazo[44;2H(B[m[38;5;60;48;5;232m48[44;5H(B[m[97;48;5;232m-# Dica: A coragem[44;24Hinabal√°vel[44;35Hdos[44;39Hfi√©is[44;45H√©[44;47Hum[44;50Haviso[44;56Hde[44;59Hjulgamento[44;70Hpara[44;75Hqu[45;2H(B[m[38;5;60;48;5;232m49[45;5H(B[m[97;48;5;232m   [45;9H  [45;12H   [45;16H  [45;19H      [45;26H [45;28H     [45;34H      [45;41H      [45;48H     [45;54H   [45;58H  [45;61H    [45;66H  [45;69H     [45;75H  [46;3H(B[m[38;5;60;48;5;232m0[46;5H(B[m[97;48;5;232mid:[46;9H053f1b0728      [46;26H [46;28H        [46;37H 